def prost(n):# получаем число
    d=2 # олучить остаток от деления на 2, значит - простое
    while n%d!=0:# пока число mod 2<>0 переберем делители, так как , простое, делится неа 2 и на себя,
        d+=1# перебрасываем все, кроме 2
        print(d==n) # выводим
    return d==n # возвращаем результат
if __name__ == '__main__':
    n=int(input())# ввод число
    print(n)# само число, в понятном виде
    if prost(n)==True:# число простое - вернуть true и напечатать вывод
        print("число простое")# по заданию просили так
    else:
        print("составное")# если оноо составное, это и вывести
